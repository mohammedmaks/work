= form_for @coach do |f|
  - if @coach.errors.any?
    #error_explanation
      %h2= "#{pluralize(@coach.errors.count, "error")} prohibited this coach from being saved:"
      %ul
        - @coach.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :coach_name
    = f.text_field :coach_name
  .actions
    = f.submit 'Save'
